<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.paola.unidad2pggs.PhotoDH">

    <ImageView
        android:layout_width="@dimen/column_size"
        android:layout_height="@dimen/column_size"
        android:src="@drawable/dh4"
        android:id="@+id/imv_thumbnail"
        android:onClick="nextActivity22"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txv_name_place"
        android:layout_centerInParent="true"
        android:text="@string/app_name"
        android:textSize="@dimen/text_size"
        android:textColor="@color/yellow"
        android:onClick="nextActivity22"
        />

</RelativeLayout>
